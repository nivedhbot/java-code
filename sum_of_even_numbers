import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int sum = 0;

        String[] input = sc.nextLine().split(" ");

        for (String str : input) {
            int number = Integer.parseInt(str);


            if (number % 2 == 0) {
                sum += number;
            }
        }

        System.out.println(sum);
    }
}
